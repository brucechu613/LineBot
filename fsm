digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	user [label=user color=red fillcolor=darksalmon peripheries=2]
	fsm [label=fsm]
	intro [label=intro]
	flirt [label=flirt]
	greet [label=greet]
	replyflirt [label=replyflirt]
	user -> fsm [label="advance [is_going_to_fsm]"]
	user -> intro [label="advance [is_going_to_intro]"]
	user -> flirt [label="advance [is_going_to_flirt]"]
	user -> greet [label="advance [is_going_to_greet]"]
	flirt -> reply_flirt [label="advance [is_going_to_reply_flirt]"]
	fsm -> user [label=go_back]
	intro -> user [label=go_back]
	greet -> user [label=go_back]
	reply_flirt -> user [label=go_back]
}
